{
    "name": "atlantis/core",
    "description": "Atlantis Framework : Core",
    "keywords": ["core","administrator", "admin", "database", "atlantis", "laravel", "cms", "enterprise"],
    "authors": [
        {
            "name": "Azri Jamil",
            "email": "azri@nematix.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "illuminate/support": "~4.2",
        "rhumsaa/uuid": "2.8.*@dev",
        "anahkiasen/former": "dev-master",
        "anahkiasen/underscore-php" : "dev-master",
        "kriswallsmith/assetic": "1.*",
        "oyejorge/less.php": "~1.5",
        "mrclay/minify": "dev-master",
        "components/jquery": "*",
        "components/jqueryui": "dev-master",
        "components/angular.js": "dev-master",
        "components/bootstrap": "dev-master",
        "components/font-awesome": "4.0.*@dev"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Atlantis\\Core\\": "src/",
            "Atlantis\\Asset\\": "src/",
            "Atlantis\\Helpers\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist",
        "component-dir": "../../../public/components",
        "component-baseurl": "/components"
    }
}
